{
  "ssh": {
    "port": 22,
    "enabled": true,
    "max_connections": 100,
    "timeout": 300
  },
  "sshws": {
    "port": 80,
    "enabled": true,
    "max_connections": 50,
    "timeout": 300
  },
  "v2ray": {
    "port": 443,
    "enabled": true,
    "protocol": "vmess",
    "uuid": "auto-generate",
    "max_connections": 200,
    "timeout": 300
  },
  "wireguard": {
    "port": 51820,
    "enabled": true,
    "interface": "wg0",
    "subnet": "10.0.0.0/24",
    "max_peers": 100
  },
  "openvpn": {
    "port": 1194,
    "enabled": true,
    "protocol": "udp",
    "subnet": "10.8.0.0/24",
    "max_clients": 100
  },
  "shadowsocks": {
    "port": 8388,
    "enabled": true,
    "method": "aes-256-gcm",
    "timeout": 300,
    "max_connections": 100
  },
  "slowdns": {
    "port": 53,
    "enabled": true,
    "upstream_dns": ["8.8.8.8", "8.8.4.4"],
    "cache_size": 1000
  },
  "udp": {
    "enabled": true,
    "ports": [53, 1194, 51820],
    "max_connections": 500
  },
  "tcp": {
    "enabled": true,
    "ports": [22, 80, 443, 8388],
    "max_connections": 500
  }
} 